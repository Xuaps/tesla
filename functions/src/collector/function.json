{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "timer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 0 * * *"
    },
    {
      "type": "blob",
      "name": "file",
      "direction": "out",
      "connection": "AzureWebJobsStorage",
      "path": "$web/prices/2.0TD/{DateTime:yyyy-MM-dd}.json",
      "dataType": "binary"
    }
  ]
}
